 => {\n            this.fetchNextGeneration()\n        }, delay);\n    }\n\n    render() {\n        console.log('this.props', this.props);\n\n        const {generationId, expiration, status, message} = this.props.generation;\n\n        if (status === fetchStates.fetching) {\n            return (\n                <div>...\n                </div>\n            );\n        }\n\n        if (status === fetchStates.error) {\n            return (\n                <div>\n                    {message}\n                </div>\n            );\n        }\n\n        return (\n            <div>\n                <h3>Generation: {generationId}. Expires on:\n                </h3>\n                <h4>{new Date(expiration).toString()}</h4>\n            </div>\n        );\n    }\n}\n\n// const mapDispatchToProps = dispatch => ({     fetchGeneration: () =>\n// fetchGeneration(dispatch) });\n\nconst mapStateToProps = state => {\n    const generation = state.generation;\n    return {generation};\n}\n\nconst componentConnector = connect(mapStateToProps, {fetchGeneration});\nexport default componentConnector(Generation);"},"lineCount":null}},"error":null,"hash":"26de74be82ee9bc23169278a3a06775b","cacheData":{"env":{}}}